;; Shortcut to compile
(global-set-key (kbd "C-x C-m") 'compile)

;; Shortcut to GDB shell command
(global-set-key (kbd "S-<f5>") 'gdb)

;; Shortcut to execute shell command
(global-set-key (kbd "<f5>") 'shell-command)

;; Shortcut to ag search
(global-set-key (kbd "C-x C-a") 'ag)

;; Shortcut to search and replace
(global-set-key (kbd "C-x C-r") 'replace-string)
